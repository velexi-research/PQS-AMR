#ifndef included_PQS_config_h
#define included_PQS_config_h

/* Macro defined if double precision library is being built. */
#ifndef PQS_DOUBLE_PRECISION
#cmakedefine PQS_DOUBLE_PRECISION
#endif

/* Floating-point precision for PQS_REAL */
#ifndef PQS_REAL
#define PQS_REAL @REAL@
#endif

/* Zero tolerance */
#ifndef PQS_ZERO_TOL
#define PQS_ZERO_TOL @ZERO_TOL@
#endif

/* Maximum value for PQS_REAL */
#ifndef PQS_REAL_MAX
#define PQS_REAL_MAX @REAL_MAX@
#endif

/* Minimum value for PQS_REAL */
#ifndef PQS_REAL_MIN
#define PQS_REAL_MIN @REAL_MIN@
#endif

/* Machine epsilon value for PQS_REAL */
#ifndef PQS_REAL_EPSILON
#define PQS_REAL_EPSILON @REAL_EPSILON@
#endif

#endif
